generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id           Int       @id @default(autoincrement())
  exchangeRate Float
  eurAmount    Float
  plnAmount    Float
  timestamp    DateTime? @default(now()) @db.Timestamp(6)
}
